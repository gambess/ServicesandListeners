<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Raziel\TestBundle\Entity\Sms" table="sms">
    <indexes>
      <index name="fk_sms_mensaje1" columns="mensaje_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="destinatario" type="string" column="destinatario" length="20" nullable="true"/>
    <field name="remitente" type="string" column="remitente" length="15" nullable="true"/>
    <field name="respuesta" type="string" column="respuesta" length="5" nullable="false"/>
    <field name="estado" type="string" column="estado" length="20" nullable="false"/>
    <field name="bitacora" type="text" column="bitacora" nullable="false"/>
    <field name="fechaEnvio" type="datetime" column="fecha_envio" nullable="true"/>
    <field name="fechaCreacion" type="datetime" column="fecha_creacion" nullable="false"/>
    <field name="fechaActualizacion" type="datetime" column="fecha_actualizacion" nullable="false"/>
    <many-to-one field="mensaje" target-entity="Mensaje">
      <join-columns>
        <join-column name="mensaje_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
