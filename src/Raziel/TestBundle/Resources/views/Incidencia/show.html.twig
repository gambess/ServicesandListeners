{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Incidencia</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
            <tr>
                <th>Fechainsercion</th>
                <td>{{ entity.fechaInsercion|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechalectura</th>
                <td>{{ entity.fechaLectura|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechaenvio</th>
                <td>{{ entity.fechaEnvio|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechalog</th>
                <td>{{ entity.fechaLog|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('incidencia') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('incidencia_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
